<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE DvfsHintList [
<!ELEMENT DvfsHintList (Hint?,TspHint+)>
<!ELEMENT Hint EMPTY>
<!ELEMENT TspHint EMPTY>
<!ATTLIST TspHint
          scenario_name CDATA #REQUIRED
          level CDATA #REQUIRED
          head CDATA #IMPLIED
          tail CDATA #IMPLIED>
]>

<DvfsHintList>
    <TspHint scenario_name="Default"            level="2" head="130 1190400 0 1" tail="500 940800 0 0" />
    <TspHint scenario_name="SipDefault"         level="1" head="130 1190400 0 0" tail="0 0 0 0" />
    <TspHint scenario_name="SipMms"             level="1" head="130 1036800 0 0" tail="0 0 0 0" />

    <Hint name="BADGE_UPDATE" timeout="2000">
        <Control type="cpu_min" value="60%"/>
    </Hint>

    <Hint name="SMOOTH_SCROLL" timeout="30000">
        <Control type="cpu_min" value="1190400"/>
    </Hint>

    <Hint name="GESTURE_DETECTED" timeout="500">
        <Control type="cpu_min" value="806400"/>
    </Hint>

    <Hint name="SUPER_TURBO" timeout="2000">
        <Control type="cpu_min" value="100%"/>
    </Hint>
    <Hint name="TURBO" timeout="2000">
        <Control type="cpu_min" value="75%"/>
    </Hint>
    <Hint name="NORMAL" timeout="2000">
        <Control type="cpu_min" value="50%"/>
    </Hint>
</DvfsHintList>
